{
  "name": "vite-plugin-themeplate",
  "title": "ThemePlate Vite",
  "version": "1.10.0",
  "description": "Straight-forward Vite integration for WordPress",
  "keywords": [
    "vite",
    "vite-plugin",
    "wordpress",
    "themeplate"
  ],
  "license": "MIT",
  "author": {
    "name": "Gene Alyson Fortunado Torcende",
    "email": "genealyson.torcende@gmail.com"
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThemePlate/Vite.git",
    "directory": "packages/js"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.8.1",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@types/node": "^22.15.0",
    "prettier": "^3.5.3",
    "rollup": "^4.40.0",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  },
  "peerDependencies": {
    "vite": "^4.0.4 || ^5.0.11 || ^6.0.7"
  },
  "scripts": {
    "test": "vitest",
    "build": "unbuild",
    "watch": "unbuild --stub",
    "prepublishOnly": "pnpm run build"
  }
}
